
<body style="background-color: #F0F0F0">
	<div class="container">
		
			<div class="panel panel-primary text-left" style="margin-left:-14px;margin-right: 12px;">
			<div class="panel-heading"><h4> Registration Details</h4></div>
				<div class="panel-body">
					<table class="table">
						<tbody>
							<tr>
								<td>Name</td>
								<td>{{user.name}}</td>
							</tr>
							<tr>
								<td>Email</td>
								<td>{{user.email}}</td>
							</tr>
							<tr>
								<td>Phone Number</td>
								<td>{{user.phone}}</td>
							</tr>
							<tr>
								<td>Age</td>
								<td>{{user.age}}</td>
							</tr>
							<tr>
							<td>Date of Birth:</td>
							<td>{{user.dt}}</td>
							</tr>
							
							<tr>
								<td>Domain/Technology</td>
								<td>{{user.tech}}</td>
							</tr>
							<tr>
								<td>Experience(years)</td>
								<td>{{user.experience}}</td>
							</tr>
						</tbody>
					</table>
					<div>
						
					</div>

				</div>
			</div>
			<a href="#/main" class="btn btn-info" role="button" style="margin-left: -15px;">Click here
							to login again</a>
		
	</div>
</body>

